<%= form_for(@attraction) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :happiness_rating %>
<%= f.number_field :happiness_rating %>
<%= f.label :nausea_rating %>
<%= f.number_field :nausea_rating %>
<%= f.label :tickets %>
<%= f.number_field :tickets %>
<%= f.submit "Update Attraction" %>
<% end %>
