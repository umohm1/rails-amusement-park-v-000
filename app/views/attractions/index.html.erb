<%= @attractions.each do |attraction| %>
  <p><%= attraction.name %></p>
  <p><%= attraction.tickets %></p>
  <% if current_user.admin %>
    <%= link_to "Show #{attraction.name}", attraction_path(attraction.id) %>
  <%else %>
    <p><%= link_to "Go on #{attraction.name}", attraction_path(attraction.id) %></p>
  <% end %>
<% end %>

<% if current_user.admin %>
<%= link_to "New Attraction", new_attraction_path  %>
<% end %>
